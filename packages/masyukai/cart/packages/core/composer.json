{
    "name": "masyukai/cart",
    "require": {
        "php": "^8.4",
        "livewire/livewire": "^3.0",
        "masyukai/cart-condition": "self.version"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Cart": "MasyukAI\\Cart\\Facades\\Cart"
            },
            "providers": [
                "MasyukAI\\Cart\\CartServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../conditions"
        }
    ],
    "autoload": {
        "psr-4": {
            "MasyukAI\\Cart\\": "src"
        }
    }
}
