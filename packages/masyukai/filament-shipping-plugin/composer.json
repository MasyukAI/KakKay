{
    "name": "masyukai/filament-shipping-plugin",
    "description": "A comprehensive Filament admin interface for managing shipments and shipping operations",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "plugin",
        "shipping",
        "logistics",
        "admin",
        "e-commerce",
        "masyukai"
    ],
    "authors": [
        {
            "name": "MasyukAI",
            "email": "info@masyukai.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "masyukai/shipping": "^1.0"
    },
    "require-dev": {
        "laravel/framework": "^12.0",
        "livewire/livewire": "^3.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "laravel/pint": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "MasyukAI\\FilamentShippingPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MasyukAI\\FilamentShippingPlugin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasyukAI\\FilamentShippingPlugin\\FilamentShippingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/pest"
        ],
        "format": [
            "vendor/bin/pint"
        ]
    },
    "version": "1.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true
}