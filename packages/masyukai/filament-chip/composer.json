{
    "name": "masyukai/filament-chip",
    "description": "A Filament admin plugin to explore CHIP data ingested by the masyukai/chip package.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "plugin",
        "chip",
        "payments",
        "admin",
        "masyukai"
    ],
    "authors": [
        {
            "name": "MasyukAI",
            "email": "info@masyukai.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../chip"
        }
    ],
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0",
        "laravel/framework": "^12.0",
        "livewire/livewire": "^3.0",
        "masyukai/chip": "@dev",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "laravel/pint": "^1.18",
        "rector/rector": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "MasyukAI\\FilamentChip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MasyukAI\\FilamentChip\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasyukAI\\FilamentChip\\FilamentChipServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/pest"
        ],
        "format": [
            "vendor/bin/pint"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
