{
    "name": "aiarmada/commerce",
    "description": "A comprehensive commerce ecosystem for Laravel 12 with cart, payments, vouchers, shipping, and inventory management",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "laravel-12",
        "shopping-cart",
        "cart",
        "e-commerce",
        "payment-gateway",
        "chip",
        "vouchers",
        "shipping",
        "inventory",
        "filament",
        "livewire",
        "jnt",
        "stock-management",
        "documents",
        "filament-plugin",
        "commerce"
    ],
    "authors": [
        {
            "name": "AIArmada",
            "email": "info@aiarmada.com"
        }
    ],
    "homepage": "https://github.com/aiarmada/commerce",
    "support": {
        "issues": "https://github.com/aiarmada/commerce/issues",
        "source": "https://github.com/aiarmada/commerce",
        "docs": "https://github.com/aiarmada/commerce/blob/main/README.md"
    },
    "require": {
        "php": "^8.4",
        "ext-curl": "*",
        "ext-json": "*",
        "akaunting/laravel-money": "^6.0",
        "filament/filament": "^4.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/contracts": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/queue": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/validation": "^12.0",
        "laravel/framework": "^12.0",
        "livewire/livewire": "^3.0",
        "spatie/laravel-package-tools": "^1.92",
        "spatie/laravel-pdf": "^1.5"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.3",
        "rector/rector": "^2.2",
        "symplify/monorepo-builder": "^11.0"
    },
    "replace": {
        "aiarmada/cart": "self.version",
        "aiarmada/chip": "self.version",
        "aiarmada/commerce-support": "self.version",
        "aiarmada/docs": "self.version",
        "aiarmada/filament-cart": "self.version",
        "aiarmada/filament-chip": "self.version",
        "aiarmada/filament-vouchers": "self.version",
        "aiarmada/jnt": "self.version",
        "aiarmada/stock": "self.version",
        "aiarmada/vouchers": "self.version"
    },
    "repositories": [
        {
            "type": "path",
            "url": "cart"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "docs"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "filament-cart"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "stock"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "vouchers"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/cart"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/chip"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/docs"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/filament-cart"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/filament-chip"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/filament-vouchers"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/jnt"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/stock"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/support"
        },
        {
            "options": {
                "symlink": true
            },
            "type": "path",
            "url": "./packages/vouchers"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "AIArmada\\Cart\\": "packages/cart/src",
            "AIArmada\\Chip\\": "packages/chip/src",
            "AIArmada\\CommerceSupport\\": "packages/support/src",
            "AIArmada\\Docs\\": "packages/docs/src",
            "AIArmada\\FilamentCart\\": "packages/filament-cart/src",
            "AIArmada\\FilamentCart\\Database\\Factories\\": "packages/filament-cart/database/factories/",
            "AIArmada\\FilamentCart\\Database\\Seeders\\": "packages/filament-cart/database/seeders/",
            "AIArmada\\FilamentChip\\": "packages/filament-chip/src",
            "AIArmada\\FilamentVouchers\\": "packages/filament-vouchers/src",
            "AIArmada\\FilamentVouchers\\Database\\Factories\\": "packages/filament-vouchers/database/factories/",
            "AIArmada\\FilamentVouchers\\Database\\Seeders\\": "packages/filament-vouchers/database/seeders/",
            "AIArmada\\Jnt\\": "packages/jnt/src",
            "AIArmada\\Stock\\": "packages/stock/src",
            "AIArmada\\Vouchers\\": "packages/vouchers/src"
        },
        "files": [
            "packages/support/src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AIArmada\\Cart\\Tests\\": "packages/cart/tests/",
            "AIArmada\\Chip\\Tests\\": "packages/chip/tests/",
            "AIArmada\\CommerceSupport\\Tests\\": "packages/support/tests/",
            "AIArmada\\Commerce\\Tests\\": "tests/src",
            "AIArmada\\Docs\\Tests\\": "packages/docs/tests/",
            "AIArmada\\FilamentCart\\Tests\\": "packages/filament-cart/tests/",
            "AIArmada\\FilamentChip\\Tests\\": "packages/filament-chip/tests/",
            "AIArmada\\FilamentVouchers\\Tests\\": "packages/filament-vouchers/tests/",
            "AIArmada\\Jnt\\Tests\\": "packages/jnt/tests/",
            "AIArmada\\Stock\\Tests\\": "packages/stock/tests/",
            "AIArmada\\Vouchers\\Tests\\": "packages/vouchers/tests/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Cart": "AIArmada\\Cart\\Facades\\Cart",
                "Chip": "AIArmada\\Chip\\Facades\\Chip",
                "Doc": "AIArmada\\Docs\\Facades\\Doc",
                "JntExpress": "AIArmada\\Jnt\\Facades\\JntExpress",
                "Stock": "AIArmada\\Stock\\Facades\\Stock",
                "Voucher": "AIArmada\\Vouchers\\Facades\\Voucher"
            },
            "providers": [
                "AIArmada\\Cart\\CartServiceProvider",
                "AIArmada\\Chip\\ChipServiceProvider",
                "AIArmada\\CommerceSupport\\SupportServiceProvider",
                "AIArmada\\Docs\\DocsServiceProvider",
                "AIArmada\\FilamentCart\\FilamentCartServiceProvider",
                "AIArmada\\FilamentChip\\FilamentChipServiceProvider",
                "AIArmada\\FilamentVouchers\\FilamentVouchersServiceProvider",
                "AIArmada\\Jnt\\JntServiceProvider",
                "AIArmada\\Stock\\StockServiceProvider",
                "AIArmada\\Vouchers\\VoucherServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "php -d memory_limit=512M vendor/bin/pest"
        ],
        "test-coverage": [
            "php -d memory_limit=512M vendor/bin/pest --coverage"
        ],
        "format": [
            "php -d memory_limit=512M vendor/bin/pint"
        ],
        "format-test": [
            "php -d memory_limit=512M vendor/bin/pint --test"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse --memory-limit=1G"
        ],
        "rector": [
            "vendor/bin/rector"
        ],
        "rector-dry": [
            "vendor/bin/rector --dry-run"
        ],
        "ci": [
            "@phpstan",
            "@rector-dry",
            "@format-test",
            "@test"
        ],
        "monorepo:merge": [
            "vendor/bin/monorepo-builder merge"
        ],
        "monorepo:validate": [
            "vendor/bin/monorepo-builder validate"
        ],
        "monorepo:bump-interdependency": [
            "vendor/bin/monorepo-builder bump-interdependency"
        ],
        "monorepo:release": [
            "vendor/bin/monorepo-builder release"
        ]
    }
}
