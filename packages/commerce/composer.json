{
    "name": "aiarmada/commerce",
    "description": "A modern, feature-rich shopping cart package for Laravel 12",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel12",
        "shopping-cart",
        "cart",
        "e-commerce",
        "aiarmada"
    ],
    "authors": [
        {
            "name": "AIArmada",
            "email": "info@aiarmada.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./packages/cart",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/docs",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/stock",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/vouchers",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/chip",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/jnt",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/filament-cart",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/filament-chip",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "akaunting/laravel-money": "^6.0",
        "spatie/laravel-package-tools": "^1.92",
        "aiarmada/cart": "@dev",
        "aiarmada/docs": "@dev",
        "aiarmada/stock": "@dev",
        "aiarmada/vouchers": "@dev",
        "aiarmada/chip": "@dev",
        "aiarmada/jnt": "@dev",
        "aiarmada/filament-cart": "@dev",
        "aiarmada/filament-chip": "@dev"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-browser": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "phpunit/phpunit": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "AIArmada\\Cart\\": "packages/cart/src",
            "AIArmada\\Chip\\": "packages/chip/src",
            "AIArmada\\Docs\\": "packages/docs/src",
            "AIArmada\\FilamentCart\\": "packages/filament-cart/src",
            "AIArmada\\FilamentChip\\": "packages/filament-chip/src",
            "AIArmada\\Jnt\\": "packages/jnt/src",
            "AIArmada\\Stock\\": "packages/stock/src",
            "AIArmada\\Vouchers\\": "packages/vouchers/src"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Cart": "AIArmada\\Cart\\Facades\\Cart"
            },
            "providers": [
                "AIArmada\\Cart\\CartServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AIArmada\\Commerce\\Tests\\": "tests/src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/pest"
        ],
        "test-coverage": [
            "vendor/bin/pest --coverage"
        ],
        "format": [
            "vendor/bin/pint"
        ],
        "format-test": [
            "vendor/bin/pint --test"
        ]
    },
    "version": "1.0.0",
    "minimum-stability": "stable",
    "prefer-stable": true
}
