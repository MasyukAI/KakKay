{
    "name": "aiarmada/commerce",
    "description": "A powerful collection of commerce components for Laravel - build and ship e-commerce features fast.",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "composer-runtime-api": "^2.1",
        "ext-intl": "*"
    },
    "require-dev": {
        "akaunting/laravel-money": "^6.0",
        "anourvalar/eloquent-serialize": "^1.2",
        "chillerlan/php-qrcode": "^5.0",
        "filament/filament": "^5.0",
        "filament/support": "*",
        "guzzlehttp/guzzle": "^7.0",
        "larastan/larastan": "^3.0",
        "laravel/framework": "^12.0",
        "laravel/pint": "^1.0",
        "laravel/prompts": "^0.3.5",
        "livewire/livewire": "^4.0@beta",
        "league/csv": "^9.27",
        "league/flysystem-aws-s3-v3": "^3.0",
        "nunomaduro/termwind": "^2.0",
        "openspout/openspout": "^4.23",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phiki/phiki": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^2.1",
        "pragmarx/google2fa": "^8.0",
        "pragmarx/google2fa-qrcode": "^3.0",
        "rector/rector": "^2.0",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-package-tools": "^1.92",
        "spatie/laravel-pdf": "^1.5",
        "spatie/laravel-ray": "^1.29",
        "spatie/laravel-settings": "^3.0",
        "spatie/laravel-tags": "^4.2",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "symplify/monorepo-builder": "^11.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/commerce-support"
        }
    ],
    "autoload": {
        "files": [
            "packages/commerce-support/src/helpers.php"
        ],
        "psr-4": {
            "AIArmada\\Cart\\": "packages/cart/src",
            "AIArmada\\Cart\\Tests\\": "packages/cart/tests/",
            "AIArmada\\Chip\\": "packages/chip/src",
            "AIArmada\\Chip\\Tests\\": "packages/chip/tests/",
            "AIArmada\\CommerceSupport\\": "packages/commerce-support/src",
            "AIArmada\\CommerceSupport\\Tests\\": "packages/commerce-support/tests/",
            "AIArmada\\Commerce\\Tests\\": "tests/src",
            "AIArmada\\Docs\\": "packages/docs/src",
            "AIArmada\\Docs\\Tests\\": "packages/docs/tests/",
            "AIArmada\\FilamentCart\\": "packages/filament-cart/src",
            "AIArmada\\FilamentCart\\Database\\Factories\\": "packages/filament-cart/database/factories/",
            "AIArmada\\FilamentCart\\Database\\Seeders\\": "packages/filament-cart/database/seeders/",
            "AIArmada\\FilamentCart\\Tests\\": "packages/filament-cart/tests/",
            "AIArmada\\FilamentChip\\": "packages/filament-chip/src",
            "AIArmada\\FilamentChip\\Tests\\": "packages/filament-chip/tests/",
            "AIArmada\\FilamentVouchers\\": "packages/filament-vouchers/src",
            "AIArmada\\FilamentVouchers\\Database\\Factories\\": "packages/filament-vouchers/database/factories/",
            "AIArmada\\FilamentVouchers\\Database\\Seeders\\": "packages/filament-vouchers/database/seeders/",
            "AIArmada\\FilamentVouchers\\Tests\\": "packages/filament-vouchers/tests/",
            "AIArmada\\Jnt\\": "packages/jnt/src",
            "AIArmada\\Jnt\\Tests\\": "packages/jnt/tests/",
            "AIArmada\\Stock\\": "packages/stock/src",
            "AIArmada\\Stock\\Tests\\": "packages/stock/tests/",
            "AIArmada\\Vouchers\\": "packages/vouchers/src",
            "AIArmada\\Vouchers\\Tests\\": "packages/vouchers/tests/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs": [
            "rector",
            "pint",
            "npm run prettier"
        ],
        "pint": "pint --config pint-strict-imports.json",
        "rector": "rector",
        "test:pest": "pest --parallel",
        "test:phpstan": "phpstan analyse",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    }
}
