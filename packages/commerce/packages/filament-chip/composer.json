{
  "name": "aiarmada/filament-chip",
  "description": "A Filament admin plugin to explore CHIP data ingested by the aiarmada/chip package.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/aiarmada/commerce",
  "support": {
    "issues": "https://github.com/aiarmada/commerce/issues",
    "source": "https://github.com/aiarmada/commerce"
  },
  "keywords": [
    "laravel",
    "filament",
    "plugin",
    "chip",
    "payments",
    "admin",
    "aiarmada"
  ],
  "authors": [
    {
      "name": "AIArmada",
      "email": "info@aiarmada.com"
    }
  ],
  "require": {
    "php": "^8.4",
    "aiarmada/commerce-support": "self.version",
    "aiarmada/chip": "self.version",
    "filament/filament": "^4.0",
    "laravel/framework": "^12.0",
    "livewire/livewire": "^3.0"
  },
  "require-dev": {
    "driftingly/rector-laravel": "^2.0",
    "larastan/larastan": "^3.0",
    "orchestra/testbench": "^10.0",
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-browser": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "pestphp/pest-plugin-type-coverage": "^4.0",
    "laravel/pint": "^1.18",
    "rector/rector": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "AIArmada\\FilamentChip\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AIArmada\\FilamentChip\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AIArmada\\FilamentChip\\FilamentChipServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "format": "vendor/bin/pint",
    "analyse": "vendor/bin/phpstan analyse"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
