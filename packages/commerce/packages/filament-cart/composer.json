{
  "name": "aiarmada/filament-cart",
  "description": "A comprehensive Filament admin interface for managing shopping carts created by the AIArmada Cart package",
  "license": "MIT",
  "homepage": "https://github.com/aiarmada/commerce",
  "support": {
    "issues": "https://github.com/aiarmada/commerce/issues",
    "source": "https://github.com/aiarmada/commerce"
  },
  "require": {
    "php": "^8.4",
    "aiarmada/cart": "self.version",
    "aiarmada/commerce-support": "self.version",
    "filament/filament": "^4.2 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "AIArmada\\FilamentCart\\": "src/",
      "AIArmada\\FilamentCart\\Database\\Factories\\": "database/factories/",
      "AIArmada\\FilamentCart\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AIArmada\\FilamentCart\\FilamentCartServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
