{
  "name": "aiarmada/docs",
  "description": "Laravel document generation package with PDF support for invoices, receipts, and more",
  "license": "MIT",
  "keywords": [
    "laravel",
    "invoice",
    "receipt",
    "document",
    "pdf",
    "spatie",
    "aiarmada"
  ],
  "authors": [
    {
      "name": "AIArmada",
      "email": "info@aiarmada.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "aiarmada/commerce-support": "self.version",
    "illuminate/database": "^12.0",
    "spatie/laravel-pdf": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "AIArmada\\Docs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AIArmada\\Docs\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AIArmada\\Docs\\DocsServiceProvider"
      ],
      "aliases": {
        "Document": "AIArmada\\Docs\\Facades\\Document"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
