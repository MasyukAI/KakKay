{
    "name": "masyukai/cart-vouchers",
    "description": "Voucher and coupon system for MasyukAI Cart",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "laravel12",
        "vouchers",
        "coupons",
        "discounts",
        "cart",
        "e-commerce",
        "masyukai"
    ],
    "authors": [
        {
            "name": "MasyukAI",
            "email": "info@masyukai.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../../"
        }
    ],
    "require": {
        "php": "^8.2",
        "masyukai/cart": "^1.0",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "laravel/pint": "^1.18",
        "phpunit/phpunit": "^12.3"
    },
    "autoload": {
        "psr-4": {
            "MasyukAI\\Cart\\Vouchers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MasyukAI\\Cart\\Vouchers\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MasyukAI\\Cart\\Vouchers\\VoucherServiceProvider"
            ],
            "aliases": {
                "Voucher": "MasyukAI\\Cart\\Vouchers\\Facades\\Voucher"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
