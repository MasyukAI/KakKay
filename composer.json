{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "aiarmada/cart": "dev-main",
        "aiarmada/chip": "dev-main",
        "aiarmada/commerce-support": "dev-main",
        "aiarmada/docs": "dev-main",
        "aiarmada/filament-cart": "dev-main",
        "aiarmada/filament-chip": "dev-main",
        "aiarmada/filament-permissions": "dev-main",
        "aiarmada/filament-vouchers": "dev-main",
        "aiarmada/jnt": "dev-main",
        "aiarmada/stock": "dev-main",
        "aiarmada/vouchers": "dev-main",
        "akaunting/laravel-money": "^6.0",
        "asmit/resized-column": "^1.1",
        "calebporzio/sushi": "^2.5",
        "fakerphp/faker": "^1.24",
        "filament/filament": "^4.2",
        "filament/spatie-laravel-media-library-plugin": "^4.0",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.12",
        "laravel/tinker": "^2.10.1",
        "livewire/flux": "^2.1.1",
        "livewire/volt": "^1.7",
        "nnjeim/world": "^1.1",
        "propaganistas/laravel-phone": "^6.0",
        "spatie/laravel-pdf": "^1.5",
        "ysfkaya/filament-phone-input": "^4.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "laradumps/laradumps": "^4.0",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "laravel/telescope": "^5.11",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan boost:update --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "phpstan": [
            "vendor/bin/phpstan analyse --memory-limit=512M"
        ],
        "analyse": [
            "@phpstan"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
